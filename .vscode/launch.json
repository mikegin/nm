{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug nm",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/nm",
            "cwd": "${workspaceFolder}",
            // "args": ["-n", "network.csv", "-t", "traffic.csv"],
            "args": ["--gui"],
            "stopAtEntry": false,
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build nm",
            "miDebuggerPath": "/usr/bin/gdb"
        }
        
    ]
}
